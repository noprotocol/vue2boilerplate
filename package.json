{
  "name": "vue2boilerplate",
  "version": "0.0.2",
  "description": "Vue2 starter for NoProtocol projects",
  "scripts": {
    "start": "npm run dev-hot-reloading",
    "dev-hot-reloading": "webpack-dev-server -d --inline --hot --history-api-fallback --content-base public --host 0.0.0.0",
    "dev-fast-recompile": "webpack-dev-server -d --history-api-fallback --content-base public --host 0.0.0.0",
    "deploy": "webpack -p --progress"
  },
  "dependencies": {
    "promise-polyfill": "^6.0.2",
    "rxjs": "^5.0.1",
    "vue": "^2.5.17",
    "vue-router": "^2.1.1",
    "vue-rx": "^2.2.0",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-core": "^6.20.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.9",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-vue-jsx": "^3.2.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^4.0.0",
    "postcss-loader": "^1.2.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
