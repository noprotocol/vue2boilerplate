{
  "name": "vue2boilerplate",
  "version": "0.0.2",
  "description": "Vue2 starter for NoProtocol projects",
  "scripts": {
    "start": "npm run dev-hot-reloading",
    "dev-hot-reloading": "webpack-dev-server -d --inline --hot --history-api-fallback --content-base public --host 0.0.0.0",
    "dev-fast-recompile": "webpack-dev-server -d --history-api-fallback --content-base public --host 0.0.0.0",
    "deploy": "webpack -p --progress"
  },
  "dependencies": {
    "rxjs": "^5.0.3",
    "vue": "^2.1.10",
    "vue-router": "^2.2.0",
    "vue-rx": "^2.3.1",
    "vuex": "^2.1.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "babel-core": "^6.22.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.0",
    "file-loader": "^0.9.0",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.1.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
